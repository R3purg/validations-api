{
  "Serilog": {
    "Using": [ "Serilog.Exceptions", "Serilog", "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "System": "Information",
        "Microsoft": "Information",
        "CORE": "Information"
      }
    },
    "Enrich": [ "FromLogContext", "WithExceptionDetails" ]
  }
}
